# Environment Variables for Terrenos Premium
# IMPORTANT: Replace the placeholder values with your actual Supabase credentials

# Supabase Configuration
SUPABASE_URL=https://zymqkwfplwlumbnevtbt.supabase.co
SUPABASE_ANON_KEY=YOUR_ACTUAL_ANON_KEY_HERE
SUPABASE_SERVICE_ROLE_KEY=YOUR_ACTUAL_SERVICE_ROLE_KEY_HERE

# Database Password - Replace with your actual database password
SUPABASE_DB_PASSWORD=YOUR_DATABASE_PASSWORD_HERE

# Flask Configuration
FLASK_ENV=development
FLASK_SECRET_KEY=terrenos-premium-secret-key-2024

